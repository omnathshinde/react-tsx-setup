		"@vitejs/plugin-react-swc": "^3.9.0",
		"eslint": "^9.25.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.4.2",
		"sass": "^1.89.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5"
