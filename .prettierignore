# Ignore node_modules
node_modules/

# Build output
build/
dist/

# Environment files
.env
.env.* 

# Logs
*.log

# Coverage directory used by testing tools
coverage/

# Temporary files
tmp/
temp/

# Public folder (often static assets, not formatted)
public/

# Ignore all minified files
*.min.js
*.min.css

# Ignore images and media
*.png
*.jpg
*.jpeg
*.gif
*.svg
*.ico
*.webp
*.mp4
*.mp3

# IDE and OS specific files
.DS_Store
.vscode/
.idea/

# Optional: ignore specific config or JSON files
*.snap
